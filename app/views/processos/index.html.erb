<p id="notice"><%= notice %></p>

<h1>Listing Processos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Inicio</th>
      <th>Final</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
 

 
    <% @processos.each do |processo| %>
     

      <tr border="1" style="width:100%">

        <td><%= processo.nome %></td>
        <td><%= processo.inicio %></td>
        <td><%= processo.final %></td>
        <td>Professor ID:<%= processo.owner  %></td>
        <td><%= link_to 'Show', processo%></td>
        <td><%= link_to 'Edit', edit_processo_path(processo) %></td>
        <td><%= link_to 'Destroy', processo, method: :delete, data: { confirm: 'Are you sure?' } %></td>

        
      </tr>
    <% end %>


  </tbody>
</table>

<br>

<%= link_to 'New Processo', new_processo_path %>
